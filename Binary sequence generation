//Binary sequence generation
#include <stdio.h>
#define M 1000
int result[M];
int n;

int check(int v, int k)
{
    return 1;
}

void solution()
{
    for (int j = 1; j <= n; j++)
        printf("%d", result[j]);
    printf("\n");
}

void Try(int k)
{
    for (int v = 0; v <= 1; v++)
    {
        if (check(v,k))
        {
            result[k] = v;
            if (k == n) solution();
            else Try(k+1);
        }
    }
}
int main(void)
{
    scanf("%d", &n);
    Try(1);
    return 0;
}
