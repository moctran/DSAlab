#include <stdio.h>
/* Write C program that reads an integer value N from stdin, prints to stdout the number Q ways to assign values 1, 2, â€¦, 9 to characters 
I, C, T, H, U, S, K (characters are assigned with different values) such that: ICT - K62 + HUST = N 

Let I = x0, C = x1, T = x2, K = x3, H = x4, U = x5, S = x6 -> 100x0+10x1+x2 + ... = N (constraint) */
int X[7];
int N;
int visited[10];
int ans;
int check (int v, int k)
{
    if (visited[v] == 1) return 0;
    if (k < 6) return 1;
    else if (X[0]*100+X[1]*10+X[2]-X[3]*100-62+X[4]*1000+X[5]*100+v*10+X[2] == N) return 1;
    else return 0;
}
void Try(int k)
{
    for (int v = 1; v<=9;v++)
    {
        if (check(v,k))
        {
            X[k] = v;
            visited[v] = 1;
            if (k == 6) ans++;
            else Try(k+1);
            visited[v] = 0;
        }
    }
}
int main(void)
{
    ans = 0;
    scanf("%d", &N);
    for (int v = 1; v <=9; v++) visited[v] = 0;
    Try(0);
    printf("%d\n", ans);
    return 0;
}
